{{title 'Portals'}}
<h1>Portals</h1>

<div class="pull-right">
  {{#link-to 'portal-create' class="btn btn-primary"}}Create Portal{{/link-to}}
</div>

<iframe src="https://www.google.com/maps/d/u/0/embed?mid=1Iw2hd57g__fAY9PugKUXtnTLRSiVzZ3B" width="100%" height="480"></iframe><br><br>

<div class="pull-right">
  {{#link-to 'portal-create' class="btn btn-primary"}}Create Portal{{/link-to}}
</div>

<div class="clearfix"/>

<div class="clearfix"/>
  <ul class="nav nav-tabs">
      <li><a data-toggle="tab" href="#known">Location Known</a></li>
      <li><a data-toggle="tab" href="#unknown">Location Unknown</a></li>
    </ul>

    <div class="tab-content">


  <div id="known" class="tab-pane active in 'Location Known'">
    <ul>
      {{#each model as |portal|}}
        {{#if portal.location_known}}
          <li>{{#link-to 'portal' portal.id}}{{portal.name}}{{/link-to}}</li>
        {{/if}}
      {{/each}}
    </ul>
  </div>

  <div id="unknown" class="tab-pane fade in 'Location Unknown'">
    <ul>
      {{#each model as |portal|}}
        {{#unless portal.location_known}}
          <li>{{#link-to 'portal' portal.id}}{{portal.name}}{{/link-to}}</li>
        {{/unless}}
      {{/each}}
    </ul>
  </div>



</div>


{{outlet}}
