<div class="card card-default profile-gallery">

{{#if this.char.profile_gallery}}
    {{#each this.char.profile_gallery as |gallery|}}
        <LinkTo @route="file" @models={{array gallery.folder gallery.name}}>
        <img src="/game/uploads/{{gallery.path}}" title="{{gallery.title}}">
        </LinkTo>
    {{/each}}
{{/if}}
<div class="clearfix"></div>
</div>
