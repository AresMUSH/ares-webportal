<div class="sheet_table">
  <div class="row">
    <div class="col-sm-12 col-md-6">
      <h1>Spells</h1>
      {{#each char.spells as |spell|}}
      <div class="row">
          <div  class="col-sm-25">
            <b>{{spell.name}}</b>
          </div>
          <div  class="col-sm-5">
            <i>Level {{spell.level}} </i>
          </div>
          <div  class="col-sm-10">
            {{spell.school}}
          </div>
      </div>
      {{/each}}
    </div>
  </div>
</div>
