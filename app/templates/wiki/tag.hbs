<h1>Tag - {{model.name}}</h1>

<h2>Pages</h2>
<ul>
{{#each model.content.pages as |page| }}
<li>{{#link-to 'wiki.page' page.id}}{{page.heading}}{{/link-to}}</li>
{{/each}}
</ul>

<h2>Scenes</h2>

<ul>
{{#each model.content.scenes as |scene| }}
<li>{{#link-to 'scene' scene.id}}{{scene.title}}{{/link-to}}</li>
{{/each}}
</ul>

<h2>Characters</h2>

<ul>
{{#each model.content.chars as |char| }}
<li><a href="/char/{{char.name}}">{{char.name}}</a></li>
{{/each}}
</ul>