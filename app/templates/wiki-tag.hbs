{{title model.name}}
<h1>Tag - {{model.name}}</h1>

<h2>Pages</h2>
<ul>
{{#each model.content.pages as |page| }}
<li><LinkTo @route="wiki-page" @model={{page.id}}>{{page.heading}}</LinkTo></li>
{{/each}}
</ul>

<h2>Scenes</h2>

<ul>
{{#each model.content.scenes as |scene| }}
<li><LinkTo @route="scene" @model={{scene.id}}>{{scene.title}}</LinkTo></li>
{{/each}}
</ul>

<h2>Characters</h2>

<ul>
{{#each model.content.chars as |char| }}
<li><LinkTo @route="char" @model={{char.name}}>{{char.name}}</LinkTo></li>
{{/each}}
</ul>